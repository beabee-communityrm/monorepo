    "yargs": "^17.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@beabee/prettier-config": "workspace:^",
    "@beabee/tsconfig": "workspace:^",
    "@types/node": "^22.15.28",
    "@types/yargs": "^17.0.33",
    "prettier": "^3.5.3"
  }
}