{
  "name": "@beabee/frontend",
  "version": "0.28.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "yarn check:vue-tsc && yarn build:main && yarn build:embed",
    "build:main": "vite build",
    "build:embed": "vite build -c vite.config.public.ts",
    "serve": "vite preview",
    "i18n": "node scripts/i18n.cjs $(git rev-parse --symbolic-full-name --abbrev-ref HEAD)",
    "check": "yarn check:vue-tsc && yarn check:eslint && yarn check:prettier",
    "check:vue-tsc": "vue-tsc --noEmit -p tsconfig.build.json",
    "check:eslint": "eslint 'src/**/*.{ts,vue}'",
    "check:prettier": "prettier -c .",
    "format": "prettier --write .",
    "generate:index": "dev-cli start generate-index --extension none --baseDir $(pwd) -p ./src/type ./src/lib ./src/utils ./src/utils/api",
    "test": "vitest run"
  },
  "dependencies": {
    "@appsignal/javascript": "^1.5.0",
    "@beabee/beabee-common": "workspace:^",
    "@beabee/client": "workspace:^",
    "@beabee/fontawesome": "workspace:^",
    "@beabee/locale": "workspace:^",
    "@beabee/vue": "workspace:^",
    "@captchafox/vue": "^1.3.0",
    "@formio/vue": "5.0.0",
    "@iframe-resizer/child": "^5.3.2",
    "@iframe-resizer/parent": "^5.3.2",
    "@maptiler/client": "^2.2.0",
    "@maptiler/geocoding-control": "^1.2.4",
    "@stripe/stripe-js": "^3.5.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.12.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "date-fns": "^4.1.0",
    "formiojs": "npm:@beabee/formiojs@^4.15.11",
    "maplibre-gl": "^3.6.2",
    "mitt": "^3.0.1",
    "otpauth": "^9.3.6",
    "slugify": "^1.6.6",
    "unplugin-vue-router": "^0.15.0",
    "vue": "^3.5.16",
    "vue-maplibre-gl": "^3.1.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/types": "^7.26.3",
    "@beabee/dev-cli": "workspace:^",
    "@beabee/prettier-config": "workspace:^",
    "@beabee/tsconfig": "workspace:^",
    "@eslint/js": "^9.28.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@rollup/plugin-replace": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.1",
    "vue-tsc": "^2.2.10"
  },
  "overrides": {
    "formiojs": "npm:@beabee/formiojs@^4.15.11"
  }
}
