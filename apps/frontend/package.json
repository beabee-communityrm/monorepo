{
  "name": "@beabee/frontend",
  "version": "0.16.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "yarn check:vue-tsc && yarn build:main && yarn build:embed",
    "build:main": "vite build",
    "build:embed": "vite build -c vite.config.public.ts",
    "serve": "vite preview",
    "check": "yarn check:vue-tsc && yarn check:eslint && yarn check:prettier",
    "check:vue-tsc": "vue-tsc --noEmit",
    "check:eslint": "eslint 'src/**/*.{ts,vue}'",
    "check:prettier": "prettier -c .",
    "format": "prettier --write .",
    "generate:index": "beabee-cli generate-index ./src/type ./src/enums ./src/lib && yarn format",
    "docker:build:latest": "docker build --build-arg REVISION=$(git rev-parse --short HEAD) --build-arg VERSION=latest -t beabee/frontend:latest -f ./Dockerfile ../../",
    "docker:build:local": "docker build --build-arg REVISION=$(git rev-parse --short HEAD) --build-arg VERSION=local -t beabee/frontend:local -f ./Dockerfile ../../",
    "docker:start:local": "docker run -d -p 3002:80 -e APP_BASE_URL=http://localhost:3001 -e API_BASE_URL=/api/1.0/ beabee/frontend:local",
    "docker:start:latest": "docker run -d -p 3002:80 -e APP_BASE_URL=http://localhost:3001 -e API_BASE_URL=/api/1.0/ beabee/frontend:latest",
    "docker:restart": "docker restart $(docker ps -a | grep beabee/frontend | awk '{print $1}')",
    "docker:push:latest": "docker push beabee/frontend:latest",
    "docker:push:dev": "docker push beabee/frontend:dev"
  },
  "dependencies": {
    "@appsignal/javascript": "^1.3.30",
    "@beabee/beabee-common": "workspace:^",
    "@beabee/locales": "workspace:^",
    "@captchafox/vue": "^1.2.0",
    "@fontsource/fira-sans": "^5.0.20",
    "@fontsource/fira-sans-condensed": "^5.0.13",
    "@fontsource/nunito-sans": "^5.0.13",
    "@fontsource/open-sans": "^5.0.28",
    "@fontsource/roboto": "^5.0.13",
    "@fontsource/roboto-slab": "^5.0.20",
    "@fontsource/rubik": "^5.0.20",
    "@fontsource/ubuntu": "^5.0.13",
    "@fontsource/work-sans": "^5.0.18",
    "@formio/vue": "5.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@maptiler/client": "^1.8.1",
    "@maptiler/geocoding-control": "^1.2.4",
    "@stripe/stripe-js": "^3.5.0",
    "@tiptap/core": "2.4.0",
    "@tiptap/extension-bold": "2.4.0",
    "@tiptap/extension-bullet-list": "2.4.0",
    "@tiptap/extension-heading": "2.4.0",
    "@tiptap/extension-italic": "2.4.0",
    "@tiptap/extension-link": "2.4.0",
    "@tiptap/extension-ordered-list": "2.4.0",
    "@tiptap/extension-strike": "2.4.0",
    "@tiptap/extension-typography": "2.4.0",
    "@tiptap/extension-underline": "2.4.0",
    "@tiptap/pm": "2.4.0",
    "@tiptap/starter-kit": "2.4.0",
    "@tiptap/vue-3": "2.4.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.7.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "color2k": "^2.0.3",
    "date-fns": "^3.6.0",
    "formiojs": "npm:@beabee/formiojs@^4.15.11",
    "iframe-resizer": "^4.4.2",
    "maplibre-gl": "^3.6.2",
    "mitt": "^3.0.1",
    "otpauth": "^9.3.1",
    "qrcode-generator": "^1.4.4",
    "slugify": "^1.6.6",
    "vue": "^3.4.29",
    "vue-accessible-color-picker": "^5.0.1",
    "vue-i18n": "^9.13.1",
    "vue-maplibre-gl": "^3.1.3",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.3.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@beabee/cli": "workspace:^",
    "@beabee/deno-node": "workspace:^",
    "@beabee/workspace": "workspace:^",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/cacheable-request": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-pages": "^0.32.2",
    "vue-tsc": "^2.0.21"
  },
  "overrides": {
    "formiojs": "npm:@beabee/formiojs@^4.15.11"
  }
}
