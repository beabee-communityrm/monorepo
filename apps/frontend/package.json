{
  "name": "@beabee/frontend",
  "version": "0.16.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "vite",
    "build": "yarn check:vue-tsc && yarn build:main && yarn build:embed",
    "build:main": "vite build",
    "build:embed": "vite build -c vite.config.public.ts",
    "serve": "vite preview",
    "i18n": "node scripts/i18n.cjs $(git rev-parse --symbolic-full-name --abbrev-ref HEAD)",
    "check": "yarn check:vue-tsc && yarn check:eslint && yarn check:prettier",
    "check:vue-tsc": "vue-tsc --noEmit",
    "check:eslint": "eslint 'src/**/*.{ts,vue}'",
    "check:prettier": "prettier -c .",
    "format": "prettier --write .",
    "generate:index": "barrelsby --config barrelsby.json",
    "presetup": "yarn presetup:env",
    "presetup:env": "cp -n .env.example .env 2>/dev/null || true"
  },
  "dependencies": {
    "@appsignal/javascript": "^1.5.0",
    "@beabee/beabee-common": "workspace:^",
    "@beabee/client": "workspace:^",
    "@beabee/locale": "workspace:^",
    "@beabee/vue": "workspace:^",
    "@captchafox/vue": "^1.3.0",
    "@formio/vue": "5.0.0",
    "@iframe-resizer/child": "^5.3.2",
    "@iframe-resizer/parent": "^5.3.2",
    "@maptiler/client": "^2.2.0",
    "@maptiler/geocoding-control": "^1.2.4",
    "@stripe/stripe-js": "^3.5.0",
    "@tiptap/core": "^2.10.4",
    "@tiptap/extension-link": "^2.10.4",
    "@tiptap/extension-typography": "^2.10.4",
    "@tiptap/extension-underline": "^2.10.4",
    "@tiptap/pm": "^2.10.4",
    "@tiptap/starter-kit": "^2.10.4",
    "@tiptap/vue-3": "^2.10.4",
    "@types/body-scroll-lock": "^3.1.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.7.9",
    "body-scroll-lock": "^4.0.0-beta.0",
    "date-fns": "^4.1.0",
    "formiojs": "npm:@beabee/formiojs@^4.15.11",
    "maplibre-gl": "^3.6.2",
    "mitt": "^3.0.1",
    "otpauth": "^9.3.6",
    "qrcode-generator": "^1.4.4",
    "slugify": "^1.6.6",
    "vue": "^3.5.13",
    "vue-accessible-color-picker": "^5.2.0",
    "vue-maplibre-gl": "^3.1.3",
    "vue-multiselect": "^3.2.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/types": "^7.26.3",
    "@beabee/prettier-config": "workspace:^",
    "@beabee/tsconfig": "workspace:^",
    "@eslint/js": "^9.17.0",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "barrelsby": "^2.8.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.2",
    "vite-plugin-pages": "^0.32.5",
    "vue-tsc": "^2.2.8"
  },
  "overrides": {
    "formiojs": "npm:@beabee/formiojs@^4.15.11"
  }
}
