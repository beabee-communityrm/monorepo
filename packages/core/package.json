{
  "name": "@beabee/core",
  "version": "0.16.0",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "yarn generate:index && yarn format && yarn build:node && yarn build:types",
    "build:types": "tsc --project ./tsconfig.json && cp ./dist/types/index.d.ts ./dist/types/index.d.cts",
    "build:node": "beabee-cli cross-build node",
    "format": "prettier --write .",
    "generate:index": "beabee-cli generate-index --ext js ./src/lib ./src/services ./src/types ./src/errors ./src/utils ./src/providers/email ./src/providers/newsletter/ ./src/providers/payment ./src/providers/payment-flow"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./src": "./src/index.ts",
    "./src/*": "./src/*.ts",
    "./package.json": "./package.json"
  },
  "imports": {
    "#models/*": "./src/models/*.js",
    "#services/*": "./src/services/*.js",
    "#types/*": "./src/types/*.js",
    "#enums/*": "./src/enums/*.js",
    "#core/defaults.json": "./src/defaults.json",
    "#core/*": "./src/*.js",
    "#errors/*": "./src/errors/*.js",
    "#utils": "./src/utils/index.js",
    "#utils/*": "./src/utils/*.js"
  },
  "devDependencies": {
    "@beabee/cli": "workspace:^",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@beabee/beabee-common": "workspace:^",
    "@beabee/config": "workspace:^",
    "@beabee/models": "workspace:^",
    "@captchafox/node": "^1.2.0",
    "@inquirer/prompts": "^3.3.2",
    "@sendgrid/mail": "^8.1.3",
    "JSONStream": "^1.3.5",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "bootstrap-sass": "^3.4.3",
    "chance": "^1.1.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "clean-deep": "^3.4.0",
    "connect-busboy": "^1.0.0",
    "connect-pg-simple": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "date-fns": "^3.6.0",
    "discourse-sso": "^1.0.5",
    "dot": "^1.1.3",
    "escape-string-regexp": "^5.0.0",
    "express": "^4.19.2",
    "express-flash": "0.0.2",
    "express-session": "^1.18.0",
    "express-winston": "^4.2.0",
    "gocardless-nodejs": "^3.25.0",
    "gunzip-maybe": "^1.4.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "juice": "^10.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "muhammara": "^4.1.0",
    "nodemailer": "^6.9.13",
    "otpauth": "^9.3.1",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pug": "^3.0.3",
    "query-string": "^7.1.3",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "slugify": "^1.6.6",
    "stripe": "^15.9.0",
    "tar-stream": "^3.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "yaml-front-matter": "^4.1.1"
  }
}
